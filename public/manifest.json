{
  "manifest_version": 3,
  "name": "Youtube Ads Skipper",
  "description": "Auto skip Youtube ads extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://www.youtube.com/*"],
      "world": "MAIN"
    }
  ]
}
